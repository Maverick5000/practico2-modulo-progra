<%= semantic_form_for @returned do |f| %>
    <%= f.inputs do %>
      <%= f.input :client, :as => :select %>
      <%= f.input :tape, :as => :select, :collection => Tape.where(available: false) %>
    <% end %>
    <%= f.actions do %>
      <%= f.action :submit, :as => :button %>
      <%= f.action :cancel, :as => :link %>
    <% end %>
  <% end %>